{
  "name": "homebridge-scout",
  "version": "0.8.1",
  "description": "A Homebridge plugin that enables HomeKit integration for the Scout security system.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "schema/**/*"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "precompile": "rm -rf lib/*",
    "schema:config": "typescript-json-schema tsconfig.json HomebridgeConfig --noExtraProps --required --strictNullChecks --out schema/config.json",
    "compile": "tsc",
    "prepare": "run-s lint compile schema:config test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jordanryanmoore/homebridge-scout.git"
  },
  "keywords": [
    "homebridge-plugin",
    "homebridge",
    "homekit",
    "scout"
  ],
  "author": "Jordan Ryan Moore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanryanmoore/homebridge-scout/issues"
  },
  "homepage": "https://github.com/jordanryanmoore/homebridge-scout#readme",
  "engines": {
    "homebridge": ">=0.4.50"
  },
  "dependencies": {
    "ajv": "^6.11.0",
    "scout-api": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.2",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5",
    "typescript-json-schema": "^0.42.0"
  }
}
